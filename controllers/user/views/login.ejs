  <h4>Login</h4>
   <form action="<%=form.url%>" method="post">
    <div>
      <label for="email">Email:
        <input type="text" name="email" placeholder="email">
      </label>
    </div>
    <div>
      <label for="password">Password:
        <input type="password" name="password">
      </label>
    </div>
    <input type="submit" name="submit" value="login">
  </form>
